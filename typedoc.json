{
  "name": "Hono Takibi",
  "entryPoints": ["packages/**/*.ts"],
  "out": "./docs/hono/public",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*.test.ts",
    "**/*.d.ts",
    "packages/hono-takibi/routes/*.ts",
    "packages/hono-takibi/vite.config.ts",
    "packages/hono-takibi/vitest.config.ts",
    "**/test/**",
    "**/tests/**",
    "**/coverage/**"
  ],
  "skipErrorChecking": true,
  "plugin": ["typedoc-plugin-mermaid"]
}
