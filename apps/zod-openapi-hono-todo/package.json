    "d:migrate": "drizzle-kit migrate",
    "p:migrate": "prisma migrate dev --name init"
  },
  "devDependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/vite-dev-server": "^0.23.0",
    "@libsql/client": "^0.15.15",
    "@types/node": "^24.9.1",
    "@typespec/http": "^1.5.0",
    "@typespec/openapi3": "^1.5.0",
    "@typespec/versioning": "^0.75.0",
    "@vitest/coverage-v8": "^4.0.3",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "hono-takibi": "^0.9.52",
    "prisma": "^6.18.0",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.1.4",
    "@prisma/client": "^6.18.0",
    "drizzle-orm": "^0.44.7",
