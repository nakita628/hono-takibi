    "d:migrate": "drizzle-kit migrate",
    "p:migrate": "prisma migrate dev --name init"
  },
  "devDependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/vite-dev-server": "^0.19.1",
    "@libsql/client": "^0.15.12",
    "@types/node": "^24.3.0",
    "@typespec/http": "^1.3.0",
    "@typespec/openapi3": "^1.3.0",
    "@typespec/versioning": "^0.73.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "hono-takibi": "^0.9.40",
    "prisma": "^6.14.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.1.0",
    "@prisma/client": "^6.14.0",
    "drizzle-orm": "^0.44.4",
