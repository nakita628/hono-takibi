{
  "name": "@apps/zod-openapi-hono-todo",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "typecheck": "tsc --noEmit",
    "prepare": "cp .env.example .env",
    "takibi": "hono-takibi",
    "tsp": "tsp compile ./typespec --emit @typespec/openapi3",
