{
  "name": "nakita628-tanstack-router-query-todo",
  "type": "module",
  "private": true,
  "scripts": {
    "vite": "vite --host",
    "dev": "wrangler dev",
    "build": "rm -rf dist && rm -rf server-build && vite build --mode client && vite build",
    "preview": "wrangler dev",
    "deploy": "pnpm build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "takibi": "hono-takibi",
    "typecheck": "tsc --noEmit",
    "push": "drizzle-kit push",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && pnpm db:migrate",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:query": "wrangler d1 execute DB --local --command"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260121.0",
    "@hono/vite-build": "^1.9.2",
    "@hono/vite-dev-server": "^0.24.1",
    "@tanstack/react-query-devtools": "^5.62.16",
    "@tanstack/react-router-devtools": "^1.157.14",
    "@tanstack/router-plugin": "^1.157.14",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typespec/http": "^1.8.0",
    "@typespec/openapi3": "^1.8.0",
    "@typespec/versioning": "^0.78.0",
    "drizzle-kit": "^0.30.4",
    "hono-takibi": "workspace:^",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "wrangler": "^4.60.0"
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.2.0",
    "@libsql/client": "^0.17.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-router": "^1.157.14",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.3",
    "hono": "^4.11.5",
    "neverthrow": "^8.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.6"
  }
}
