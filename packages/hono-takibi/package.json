{
  "name": "hono-takibi",
  "description": "Hono Takibi is a CLI tool that generates Hono routes from OpenAPI specifications.",
  "version": "0.9.60",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "hono",
    "openapi"
  ],
  "homepage": "https://github.com/nakita628/hono-takibi",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakita628/hono-takibi.git"
  },
  "bugs": {
    "url": "https://github.com/nakita628/hono-takibi/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "hono-takibi": "dist/index.js"
  },
  "exports": {
    "./vite-plugin": {
      "types": "./dist/vite-plugin/index.d.ts",
      "import": "./dist/vite-plugin/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./zod-openapi-hono": {
      "types": "./dist/generator/zod-openapi-hono/openapi/index.d.ts",
      "import": "./dist/generator/zod-openapi-hono/openapi/index.js"
    },
    "./rpc": {
      "types": "./dist/core/rpc.d.ts",
      "import": "./dist/core/rpc.js"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "deps": "rm -rf node_modules && pnpm install",
    "build": "rm -rf ./dist/* && tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "pnpm build && pnpm publish"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@typespec/compiler": "^1.5.0",
    "@typespec/openapi3": "^1.5.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@hono/zod-openapi": "1.1.4",
    "@types/node": "^24.9.1",
    "@typespec/http": "^1.5.0",
    "@typespec/rest": "^0.75.0",
    "@typespec/versioning": "^0.75.0",
    "@vitest/coverage-v8": "^4.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.3",
    "zod": "^4.1.12"
  }
}
