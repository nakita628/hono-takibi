{
  "name": "hono-takibi",
  "description": "Hono Takibi is a code generator from OpenAPI to @hono/zod-openapi",
  "version": "0.9.92",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "hono",
    "openapi"
  ],
  "homepage": "https://github.com/nakita628/hono-takibi",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakita628/hono-takibi.git"
  },
  "bugs": {
    "url": "https://github.com/nakita628/hono-takibi/issues"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "hono-takibi": "dist/index.js"
  },
  "exports": {
    "./vite-plugin": {
      "types": "./dist/vite-plugin/index.d.ts",
      "import": "./dist/vite-plugin/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./zod-openapi-hono": {
      "types": "./dist/generator/zod-openapi-hono/openapi/index.d.ts",
      "import": "./dist/generator/zod-openapi-hono/openapi/index.js"
    },
    "./rpc": {
      "types": "./dist/core/rpc/index.d.ts",
      "import": "./dist/core/rpc/index.js"
    },
    "./swr": {
      "types": "./dist/core/swr/index.d.ts",
      "import": "./dist/core/swr/index.js"
    },
    "./tanstack-query": {
      "types": "./dist/core/tanstack-query/index.d.ts",
      "import": "./dist/core/tanstack-query/index.js"
    },
    "./svelte-query": {
      "types": "./dist/core/svelte-query/index.d.ts",
      "import": "./dist/core/svelte-query/index.js"
    },
    "./vue-query": {
      "types": "./dist/core/vue-query/index.d.ts",
      "import": "./dist/core/vue-query/index.js"
    },
    "./type": {
      "types": "./dist/core/type/index.d.ts",
      "import": "./dist/core/type/index.js"
    },
    "./docs": {
      "types": "./dist/core/docs/index.d.ts",
      "import": "./dist/core/docs/index.js"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "deps": "rm -rf node_modules && pnpm install",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "release": "pnpm build && pnpm publish"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@typespec/compiler": "^1.9.0",
    "@typespec/openapi3": "^1.9.0",
    "oxfmt": "^0.31.0",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@typespec/http": "^1.9.0",
    "@typespec/rest": "^0.79.0",
    "@typespec/versioning": "^0.79.0",
    "tsdown": "^0.20.3"
  }
}
